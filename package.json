{
	"name": "codetowin",
	"displayName": "Code To Win",
	"description": "project",
	"publisher": "Arpan",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.46.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:codetowin.newContest","onCommand:codetowin.compileRun"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "codetowin.newContest",
				"title": "New Contest",
				"category": "Code To Win"
			},
			{
				"command": "codetowin.compileRun",
				"title": "Compile And Run",
				"category": "Code to Win"
			}
		],
		"configuration":{
			"title": "Code To WIn",
			"properties": {
				"codetowin.terminal":{
					"type":"string",
					"default":"cmd",
					"enum":["cmd","bash"],
					"enumDescriptions":[
						"For Windows Operating System",
						"For Linux and MacOS"
					]
				},
				"codetowin.compilerPath":{
					"type":"string",
					"default":"Location to compiler directory's bin folder",
					"format": "uri"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.46.0",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"puppeteer": "^4.0.1"
	}
}
